<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.javatpoint.goodfeet"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- android:icon="@drawable/ic_launcher" -->

        <activity
            android:name="com.javatpoint.goodfeet.MainActivity"
            android:label="@string/app_name" 
              android:screenOrientation="landscape" 
            android:configChanges="keyboardHidden|orientation"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        <activity
            android:name="com.javatpoint.goodfeet.New_customer"
            android:label="@string/title_activity_new_customer" >
        </activity>
        <activity
            android:name="com.javatpoint.goodfeet.Fetchdata"
            android:label="@string/title_activity_fetchdata" >
        </activity>
        <activity
            android:name="com.javatpoint.goodfeet.Welcome"
           android:screenOrientation="landscape" 
            android:configChanges="keyboardHidden|orientation"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen" >
           
        </activity>
        <activity android:name="com.javatpoint.goodfeet.Login" >
        </activity>
        <activity android:name="com.javatpoint.goodfeet.Lookup_crm" >
        </activity>
    </application>

</manifest>